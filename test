import requests
import json

# Define the OpenSea API endpoint URL
url = "https://api.opensea.io/api/v1/collection/post-by-0x8a709cb2f722512833313b393adbe27a968dd9-2"
headers = {
    "accept": "application/json",
    "X-API-KEY": "609c816a67654d2fa8413c364469a8c1"
}
# Send a GET request to the OpenSea API
response = requests.get(url, headers=headers)

# Convert the API response to JSON format
json_data = json.loads(response.text)

# Access specific fields within the JSON data
collection_name = json_data["assets"][0]["collection"]["name"]
collection_description = json_data["assets"][0]["collection"]["description"]
collection_image_url = json_data["assets"][0]["collection"]["image_url"]

# Print the collection name, description, and image URL
print("Collection Name: ", collection_name)
print("Collection Description: ", collection_description)
print("Collection Image URL: ", collection_image_url)

